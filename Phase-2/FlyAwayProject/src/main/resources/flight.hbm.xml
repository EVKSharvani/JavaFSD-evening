<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping package="com.simplillearn">
    	<class name="FlightDetails" table="	Flight">
    		<id name="fno" type="long" column="FNO">
    			<generator class="identity"></generator>
    		</id>
    		<property name="airline" type="string" column="AIRLINE"></property>
    		<property name="src" type="string" column="SRC"></property>
    		<property name="des" type="string" column="DES"></property>
    		
    		
    		
    	</class>
    </hibernate-mapping>